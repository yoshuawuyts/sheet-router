{
  "name": "sheet-router",
  "version": "4.0.0-0",
  "description": "Fast, modular client router",
  "main": "dist/bundle.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm run build && standard && NODE_ENV=test tape test/*",
    "test:cov": "standard && NODE_ENV=test istanbul cover test/*",
    "build": "rollup --config"
  },
  "repository": "yoshuawuyts/sheet-router",
  "keywords": [
    "client",
    "router",
    "modular",
    "wayfarer",
    "history",
    "pushstate",
    "single-page",
    "spa",
    "progressive"
  ],
  "license": "MIT",
  "dependencies": {
    "global": "^4.3.0",
    "pathname-match": "^1.1.2",
    "wayfarer": "^6.2.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "dependency-check": "^2.5.1",
    "istanbul": "^0.4.1",
    "jsdom": "^9.4.2",
    "noop2": "^2.0.0",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-browserify-transform": "^0.1.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "standard": "^5.4.1",
    "tape": "^4.4.0",
    "unassertify": "^2.0.3",
    "virtual-dom": "^2.1.1"
  }
}
